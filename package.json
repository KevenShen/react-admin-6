{
  "name": "my-react-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:docker": "vite build && yarn copy && yarn push && yarn docker:build && yarn docker:admin && yarn gulp",
    "build:push": "vite build && yarn copy && yarn push && yarn gulpdocker",
    "build": "vite build",
    "copy": "node copy.js",
    "push": "start ./push.sh",
    "docker:build": "docker build -t admin .",
    "docker:admin": "docker save admin -o admin.tar",
    "gulp": "gulp deploy",
    "gulpdocker": "gulp --gulpfile gulpfile-docker.js deploy",
    "preview": "vite preview",
    "format": "prettier --write .",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "server": "cd ../nestJs && yarn start:dev"
  },
